<h4>Settings Page</h4>
<div class="table-responsive">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Name</th>
        <th>Position</th>
        <th>Email</th>
        <th>Phone</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      {{#each}}
      <tr>
        <td>{{name}}</td>
        <td>{{position}}</td>
        <td>{{email}}</td>
        <td>{{phone}}</td>
        <td><a {{action "deleteDailyMember" this}} style="text-decoration:none" class="glyphicon glyphicon-remove"</a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
